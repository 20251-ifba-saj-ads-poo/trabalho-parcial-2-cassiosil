<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/24" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifba.saj.fwads.controller.ListarSolicitacaoController">
   <center>
      <TableView fx:id="tblSolicitacao" prefHeight="347.0" prefWidth="489.0">
        <columns>
          <TableColumn fx:id="columnEquipamento" editable="false" minWidth="-1.0" prefWidth="201.0" text="Equipamento" />
          <TableColumn fx:id="columnFuncionario" editable="false" minWidth="-1.0" prefWidth="173.0" text="Funcionario" />
            <TableColumn fx:id="columnDataSolicitacao" editable="false" minWidth="-1.0" prefWidth="225.0" text="Data de solicitacao" />
            <TableColumn fx:id="columnDataDevolucao" editable="false" minWidth="-1.0" prefWidth="225.0" text="Data de devolução" />
            <TableColumn fx:id="columnStatus" editable="false" minWidth="-1.0" prefWidth="225.0" text="Status" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" styleClass="main" stylesheets="@assets/style.css" BorderPane.alignment="CENTER">
        <buttons>
            <Button mnemonicParsing="false" onAction="#showNovaSolicitacao" styleClass="bottom" stylesheets="@assets/style.css" text="Nova Solicitacao" textFill="WHITE" />
        </buttons>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
</BorderPane>
